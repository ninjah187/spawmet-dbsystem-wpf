<Window x:Class="SpawmetDatabaseWPF.AddPartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddPartWindow" Height="300" Width="300" ResizeMode="CanMinimize">
    <DockPanel Margin="10">
        <StackPanel DockPanel.Dock="Left">
            <TextBlock Text="Nazwa:"></TextBlock>
            <TextBlock Text="Ilość:" Margin="0, 10"></TextBlock>
        </StackPanel>
        
        <StackPanel DockPanel.Dock="Right">
            <TextBox x:Name="NameTextBox" Margin="5,0"></TextBox>
            <TextBox x:Name="AmountTextBox" Margin="5,5"></TextBox>
            <Button Width="60" Click="ButtonBase_OnClick">Dodaj</Button>
        </StackPanel>
    </DockPanel>
</Window>
