<Window x:Class="SpawmetDatabaseWPF.SaveFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Zapisywanie do pliku" SizeToContent="WidthAndHeight"
        Icon="icon2.ico">
    <StackPanel>
        <TextBlock x:Name="TitleTextBlock" Text="Zapisywanie..." TextAlignment="Center" Margin="0,10"></TextBlock>
        <ProgressBar x:Name="SaveProgressBar" MinWidth="300" Height="15" Margin="0,0,0,20" IsIndeterminate="True"></ProgressBar>
    </StackPanel>
</Window>
